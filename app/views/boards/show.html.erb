<section id = "form">
  <div class= "container">
    <div class="col-sm-8 col-sm-offset-2 new_board">
      <div class="panel panel-default">
        <div class="panel-heading">
          <h2 style="text-align: center;"><%= "#{current_user.name} created board" %></h2>
        </div>
        <div class="panel-body">
          <div class="col-sm-6 col-sm-offset-4">
						<p>
						  <b>Title:</b>
						  <%= @board.title %>
						</p>
						<p>
						  <b>Team:</b>
						  <%# @board_teams.each do |board_team| %>
						  	<%= @team.name %>
						  <%# end %>
						</p>
					<%= link_to 'Edit', edit_user_board_path(current_user, @board) %>
					<%= link_to 'Back', users_path %>
					</div>
				</div>
      </div>
    </div>
  </div>
</section>